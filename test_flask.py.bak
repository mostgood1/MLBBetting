from flask import Flask, jsonify
from datetime import datetime

print("Starting minimal Flask test...")

app = Flask(__name__)

print("Flask app created successfully")

@app.route('/api/test')
def test_route():
    print("Test route accessed!")
    return jsonify({
        'success': True,
        'message': 'Minimal Flask test working',
        'timestamp': datetime.now().isoformat()
    })

print("Route defined successfully")

@app.route('/api/betting-recommendations-analysis')
def test_betting_analysis():
    print("Betting analysis route accessed!")
    return jsonify({
        'success': True,
        'message': 'Test betting analysis route working',
        'timestamp': datetime.now().isoformat()
    })

print("Betting analysis route defined successfully")

if __name__ == '__main__':
    print("Starting Flask server...")
    app.run(host='0.0.0.0', port=5001, debug=True)
