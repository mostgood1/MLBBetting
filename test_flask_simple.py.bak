#!/usr/bin/env python3

from flask import Flask, jsonify

app = Flask(__name__)

print("DEBUG: Creating test Flask app")

@app.route('/api/test')
def api_test():
    return jsonify({'message': 'Test route works'})

@app.route('/api/betting-recommendations-analysis')
def api_betting_recommendations_analysis():
    return jsonify({
        'success': True,
        'message': 'Betting recommendations analysis endpoint working',
        'debug': 'This is the minimal test version'
    })

print("DEBUG: Routes defined, starting Flask app")

if __name__ == '__main__':
    print("Available routes:")
    for rule in app.url_map.iter_rules():
        print(f"  {rule.rule} -> {rule.endpoint}")
    
    print("\nStarting server on port 5002...")
    app.run(debug=True, port=5002)
